  "scripts": {
    "dev": "NODE_OPTIONS='--no-deprecation' next dev --turbo",
    "build": "tsx lib/db/migrate && NODE_OPTIONS='--no-deprecation' next build",
    "start": "NODE_OPTIONS='--no-deprecation' next start",
    "lint": "pnpm dlx ultracite@latest check",
